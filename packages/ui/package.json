    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "class-variance-authority": "^0.7.0",
    "install": "^0.13.0",
    "npm": "^10.8.1",
